									"\r",
									"if (jsonData.accessToken) {\r",
									"    pm.collectionVariables.set(\"accessToken\", jsonData.accessToken);\r",
									"    console.log(\"Token guardado:\", jsonData.accessToken);\r",
									"} else {\r",
									"    console.warn(\"No se encontró 'accessToken' en la respuesta.\");\r",
									"}\r",
